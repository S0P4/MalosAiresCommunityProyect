stock GetRGBAVehicleColor(color) {
	return (0 <= color <= 255) ? (VehicleColoursTableRGBA[color]) : (0x000000FF);
}

stock GetEmbeddedVehicleColor(color)
{
	new str[9];

	if(!(0 <= color <= 255)) {
		color = 0;
	}

	strcat(str, VehicleColoursTableEmbedded[color], sizeof(str));
	return str;
}